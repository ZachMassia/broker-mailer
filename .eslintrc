{
  "extends": "eslint-config-airbnb",
  "rules": {
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": [
      2,
      {
        "ignore": [
          "electron"
        ]
      }
    ],
    "linebreak-style": 0,
    "react/jsx-one-expression-per-line": {
      "allow": "literal"
    },
    "react/prefer-stateless-function": 0
  },
  "parser": "babel-eslint"
}
